<div class="medium-8 columns medium-centered text-center">
	<h2>Listing users</h2>
</div>



<div class="medium-8 medium-centered columns">
	<table class="medium-12">
		<tr>
			<th class="medium-6 columns">Players:</th>
			<th class="medium-6 columns">Interested in:</th>
		</tr>
		<% @users.each do |user| %>
			<tr>
				<td class="medium-6 columns"><%= user.nick %></td>
				<td class="medium-6 columns"><%= user.game %></td>
			</tr>
		<% end %>
	</table>
</div>
